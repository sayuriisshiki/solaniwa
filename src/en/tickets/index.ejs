<%
var lang = 'en';
var topDir = '/'+lang+'/';
%>
<!DOCTYPE html>
<html lang="<%- lang %>">
<head>
    <%- include('../../_include/meta_' + lang, {title:'Ticket'}) %>
    <%- include('../../_include/style', {lang:lang}) %>

    <%- include('../../_include/analytics') %>
</head>
<body class="p-tickets p-lower">

<%- include('../../_include/header_' + lang, {lower:1}) %>

<div id="main">

    <div class="category">
        <div class="sec-title">Advanced Ticket</div>
        <div class="category-list">
            <a href="admission.html" class="category-item">
                <picture>
                    <source media="(max-width: 768px)" srcset="/assets/images/tickets/top/image-1-sp.jpg">
                    <img src="/assets/images/tickets/top/image-1.jpg" alt="">
                </picture>
                <p class="category-name">Admission Tickets</p>
            </a>
            <a href="bath.html" class="category-item">
                <picture>
                    <source media="(max-width: 768px)" srcset="/assets/images/tickets/top/image-2-sp.jpg">
