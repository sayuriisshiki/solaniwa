                <p class="category-name">Relaxation</p>
            </a>
            <a href="beauty.html" class="category-item">
                <picture>
                    <source media="(max-width: 768px)" srcset="/assets/images/tickets/top/image-6-sp.jpg">
                    <img src="/assets/images/tickets/top/image-6.jpg" alt="">
                </picture>
                <p class="category-name">Beauty</p>
            </a>
        </div>
    </div>

    <%- include('../../_include/magazine_' + lang) %>
    <%- include('../../_include/footer_' + lang, {lower:1}) %>
    <%- include('../../_include/navFix_' + lang) %>
